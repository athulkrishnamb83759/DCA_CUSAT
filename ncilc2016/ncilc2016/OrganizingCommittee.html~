    	  <script type="text/javascript">
	         $( document ).ready(function(){
			 
			         $(".button-collapse").sideNav();
					  $('ul.tabs').tabs();
			 
			 });
			 
			 
	var i=0;

		var myVar=setInterval(function(){myTimer()},1000);

function myTimer() {	 
	  $('#clock-m').countdown('2016/01/16 09:00:00', function(event) {
			  
			
   var $this = $(this).html(event.strftime('<span>%w</span><span> weeks &nbsp;&nbsp;</span>'
     + '<span>%d</span><span> days  &nbsp;&nbsp;</span>'
     + '<span>%H</span><span >hr  &nbsp;&nbsp;</span>'
     + '<span>%M</span><span> min  &nbsp;&nbsp;</span>'
     + '<span>%S</span><span> sec  &nbsp;&nbsp;</span> until the conference.'
	 
	 ));
 });
 
			 
}			 
			 
			 
			 
			 
	   
	  </script>

